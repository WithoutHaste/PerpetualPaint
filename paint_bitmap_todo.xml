          <Data ss:Type="String">Task</Data>
        </Cell>
        <Cell ss:StyleID="cellShortDate">
          <Data ss:Type="DateTime">2018-10-23T00:00:00.000</Data>
        </Cell>
      </Row>
      <Row>
        <Cell>
          <Data ss:Type="Number">13</Data>
        </Cell>
        <Cell ss:StyleID="paragraph">
          <Data ss:Type="String">a selected region (even if not grouped) can be moved around the image and dropped elsewhere</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">Todo</Data>
