      <Row>
        <Cell>
          <Data ss:Type="Number">10</Data>
        </Cell>
        <Cell ss:StyleID="paragraph">
          <Data ss:Type="String">shift-arrow-number_keys-x_key-number_keys-enter will select the specified rectangle of bits in the indicated direction (first dimension) and the clockwise direction of that for the 2nd dimension</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">Todo</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">Task</Data>
        </Cell>
        <Cell ss:StyleID="cellShortDate">
          <Data ss:Type="DateTime">2018-10-23T00:00:00.000</Data>
        </Cell>
