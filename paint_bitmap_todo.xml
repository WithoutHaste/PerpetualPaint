      <Row>
        <Cell>
          <Data ss:Type="Number">12</Data>
        </Cell>
        <Cell ss:StyleID="paragraph">
          <Data ss:Type="String">a selected region can be grouped
- edit as normal, but it is selected as a group</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">Todo</Data>
        </Cell>
